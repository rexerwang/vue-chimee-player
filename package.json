{
  "name": "vue-chimee-player",
  "version": "0.2.0",
  "description": "chimee player component/directive for vue",
  "main": "src/directive.js",
  "browser": "dist/vue-chimee-player.umd.min.js",
  "files": [
    "src",
    "dist/*.js",
    "dist/*.map",
    "dist/*.css"
  ],
  "scripts": {
    "example": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib src/directive.js",
    "lint": "vue-cli-service lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rexerwang/vue-chimee-player.git"
  },
  "keywords": [
    "vue",
    "chimee",
    "video-player"
  ],
  "author": "Rexer Wang",
  "license": "MIT",
  "dependencies": {
    "deepmerge": "^2.2.1"
  },
  "peerDependencies": {
    "vue": "^2.5.17",
    "chimee-player": "latest"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.5",
    "@vue/cli-plugin-eslint": "^3.0.5",
    "@vue/cli-service": "^3.0.5",
    "@vue/eslint-config-prettier": "^3.0.5",
    "vue": "^2.5.17",
    "chimee-player": "latest",
    "node-sass": "^4.9.4",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "prettier": {
    "singleQuote": true,
    "semi": false
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
